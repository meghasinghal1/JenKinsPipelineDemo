pipeline {
    agent any

    stages {
    	 stage('HelloTest') {
            steps {
                echo "hello world before"
		echo "${env.JOB_NAME}"
		echo "hello world after"
            }
        }
	stage('HelloPSTest') {
            steps {
	    	powershell 'write-host "hello PS word"'
            }
        }
	stage('HelloTest1') {
            steps {
                echo "hello world1"
            }
        }
    }
}
