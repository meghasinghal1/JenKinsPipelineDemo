pipeline {
    agent any

    stages {
    	 stage('HelloTest') {
            steps {
                echo "hello world"
            }
        }
	stage('HelloPSTest') {
            steps {
	    	powershell '''
			write-host "hello world"
			write-output "hello world"
		'''
            }
        }
	stage('HelloTest1') {
            steps {
                echo "hello world1"
            }
        }
    }
}
